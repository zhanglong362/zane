6. 自定义异常
    class RegisterError(BaseException):
        def __init__(self, msg, user):
            self. msg = msg
            self.user = user

        def __str__(self):
            return '<%s %s>' % (self.user, self.msg)

    raise RegisterError('注册失败', 'teacher')

